GET         /assets/*file                                               controllers.Assets.at(path="/public", file)

GET         /                                                           controllers.HomeController.index()

GET         /start                                                      controllers.HomeController.start()

GET         /robots.txt                                                 controllers.HomeController.robots()

GET         /wip                                                        controllers.Default.todo

GET         /search-payment-practice-reports/start                      controllers.SearchController.start()
GET         /search-payment-practice-reports                            controllers.SearchController.search(q:Option[String], page:Option[Int], items_per_page:Option[Int])
GET         /search-payment-practice-reports/company/:id/reports        controllers.SearchController.company(id:CompaniesHouseId, page:Option[Int])
GET         /search-payment-practice-reports/report/:reportId           controllers.SearchController.view(reportId: ReportId)

GET         /download-payment-practice-data                             controllers.DownloadController.show
GET         /download-payment-practice-data/export                      controllers.DownloadController.export
